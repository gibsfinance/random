{
  "name": "@gibs/random-provider",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "db:prepare": "npm run db:start && npm run db:migrate-up",
    "db:start": "docker compose up -d gibrandom-postgres",
    "db:stop": "docker compose down",
    "docker:provide": "docker compose down gibrandom-provide && docker compose up -d gibrandom-provide",
    "db:migrate-down-all": "tsx ./node_modules/knex/bin/cli.js migrate:rollback --all",
    "db:migrate-down": "tsx ./node_modules/knex/bin/cli.js migrate:down",
    "db:migrate-rollback": "tsx ./node_modules/knex/bin/cli.js migrate:rollback",
    "db:migrate-reset": "npm run db:migrate-down-all && npm run db:migrate-latest",
    "db:migrate-up": "tsx ./node_modules/knex/bin/cli.js migrate:up",
    "db:migrate-latest": "tsx ./node_modules/knex/bin/cli.js migrate:latest",
    "db:migrate-make": "tsx ./node_modules/knex/bin/cli.js migrate:make --esm",
    "db:seed-make": "tsx ./node_modules/knex/bin/cli.js seed:make --esm",
    "db:seed": "tsx ./node_modules/knex/bin/cli.js seed:run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "provide:dev": "bun run generate && bun --watch ./src/bin/provide",
    "provide": "bun run generate && bun ./src/bin/provide",
    "bleach:dev": "tsx --watch ./src/bin/bleach",
    "bleach": "tsx ./src/bin/bleach",
    "withdraw:dev": "bun --watch ./src/bin/withdraw",
    "withdraw": "tsx ./src/bin/withdraw",
    "consumer-bot:dev": "bun --watch ./src/bin/consumer-bot",
    "consumer-bot": "bun ./src/bin/consumer-bot",
    "generate": "bun gql-gen generate --config codegen.config.cjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@gibs/random": "^0.0.16",
    "@ponder/utils": "^0.1.7",
    "@scure/bip32": "^1.4.0",
    "@scure/bip39": "^1.3.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.0",
    "@types/pg": "^8.11.6",
    "ethers": "^6.13.2",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "pg": "^8.12.0",
    "promise-limit": "^2.7.0",
    "viem": "^2.20.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "tsx": "^4.19.0"
  }
}
